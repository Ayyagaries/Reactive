{
  "name": "Reactive",
  "description": "Reactive - a React Native App Skeleton",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Sebastian Mysakowski",
    "email": "sebastian.mysakowski@gmail.com",
    "url": "https://github.com/SMysakowski"
  },
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.12.0",
    "yarn": ">=1.21.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SMysakowski/Reactive.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx ./",
    "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.62.1",
    "react-native-svg": "12.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/runtime": "7.9.2",
    "@react-native-community/eslint-config": "1.0.0",
    "@types/jest": "25.2.1",
    "@types/react-native": "0.62.1",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-jest": "25.2.6",
    "babel-plugin-module-resolver": "4.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "7.2.1",
    "eslint-config-prettier": "6.10.1",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "husky": "4.2.5",
    "jest": "25.2.7",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "2.0.4",
    "pretty-quick": "2.0.1",
    "react-test-renderer": "16.13.1",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn run lint"
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [
    "react",
    "react-native",
    "typescript",
    "eslint",
    "detox",
    "react-navigation"
  ]
}
